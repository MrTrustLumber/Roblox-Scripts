  makesound("rbxassetid://156785206")
    if GAxe == true then
        GAxe = false
  Goldaxe.Text = "Gold Axe Off"
Goldaxe.TextColor3 = Color3.new(255, 0, 0)
    elseif GAxe == false then
        GAxe = true
  Goldaxe.Text = "Gold Axe On"
Goldaxe.TextColor3 = Color3.new(0, 1, 0)
--Credits to Johiro
    end
end)

