function clr(num1, num2, num3) --color3 converter function
	local color1 = num1/255
	local color2 = num2/255
	local color3 = num3/255
	return Color3.new(color1, color2, color3)
end

textcolor = clr(255,255,255)
textcolor2 = clr(0,0,0)

