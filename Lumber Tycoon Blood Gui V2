for i,v in pairs(getreg()) do 
   if typeof(v) == "function" and getfenv(v).script == game.Players.LocalPlayer.PlayerGui.LoadSaveGUI.LoadSaveClient.LocalScript then 
       hookfunction(v, function() end)
   end
end

