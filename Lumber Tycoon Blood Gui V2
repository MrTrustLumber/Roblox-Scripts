game.Workspace.Ferry.TimeToDeparture.Changed:connect(function()
Depart.Text = "Ferry Departs: ".. game.Workspace.Ferry.TimeToDeparture.Value
if Depart.Text == "Ferry Departs: 0" then 
	wait(6)
	Depart.Text = "Ferry has Departed"
else
	Depart.Text = "Ferry Departs: ".. game.Workspace.Ferry.TimeToDeparture.Value
end
end)

